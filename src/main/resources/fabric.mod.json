{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "https://github.com/Cubenyx-Studio/minecraft-overlay",
    "sources": "https://github.com/Cubenyx-Studio/minecraft-overlay",
    "issues": "https://github.com/Cubenyx-Studio/minecraft-overlay/issues"
  },
  "license": "All Rights Reserved",
  "icon": "logo.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.cubenyxstudio.minecraftoverlay.MinecraftOverlay"
    ]
  },
  "mixins": [
    "minecraftoverlay.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "1.21.x",
    "java": ">=21",
    "fabric-api": "*"
